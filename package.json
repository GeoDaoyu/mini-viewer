{
  "name": "mini-viewer",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/mini-viewer.umd.cjs",
  "module": "./dist/mini-viewer.js",
  "types": "./index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/mini-viewer.js",
    "require": "./dist/mini-viewer.umd.cjs"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite build --outDir demo --emptyOutDir && cp public/demo.html demo/index.html"
  },
  "devDependencies": {
    "typescript": "~5.9.2",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@geodaoyu/accessor": "^3.0.2"
  }
}
